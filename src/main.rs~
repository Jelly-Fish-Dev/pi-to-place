
use std::str::FromStr;
use std::env;
use std::fs;

fn main() {
    let mut points: usize;
	points = 0;
    for arg in std::env::args().skip(1){
		points = usize::from_str(&arg).expect("error parsing number");
	}

	let pi = fs::read_to_string("pi.txt").expect("cannot read files");
	println!("{}", &pi[0..points+2]);
}
